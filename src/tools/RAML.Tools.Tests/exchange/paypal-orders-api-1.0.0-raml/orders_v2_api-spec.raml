  purchase_unit-json:
    description: The purchase unit details. Used to capture required information for the payment contract.
    type: object
    properties:
      reference_id:
        description: The API caller-provided external ID for the purchase unit. Required for multiple purchase units when you must update the order through `PATCH`.
        maxLength: 256
        type: string
      amount:
        type: MerchantsCommonComponentsSpecification-v1-schema-amount_with_breakdown-json
      payee:
        description: The merchant who receives payment for this transaction.
        type: MerchantsCommonComponentsSpecification-v1-schema-payee-json
      payment_instruction:
        type: MerchantsCommonComponentsSpecification-v1-schema-payment_instruction-json
      description:
        description: The purchase description.
        maxLength: 127
        type: string
      custom_id:
        description: The API caller-provided external ID. Used to reconcile API caller-initiated transactions with PayPal transactions. Appears in transaction and settlement reports.
        maxLength: 127
        type: string
      invoice_id:
        description: The API caller-provided external invoice ID for this order.
        maxLength: 127
        type: string
      id:
        description: 'PayPal Generated identifier for the purchase_unit. This identifier appears in both the payer''s transaction history and the emails that the payer receives. In addition, this identifier will be available in transaction/settlement reports that can be used by merchants/API Caller to reconcile transaction. This identifier is only available when an order is saved by calling v2/checkout/orders/id/save.'
