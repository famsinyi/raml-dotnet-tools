#%RAML 1.0
title: Maps
version: 1.0
baseUri: http://maps.com/api
mediaType: application/json
types:
    MapOfInt: 
        type: object
        properties:
            []:
                type: integer
    Person:
        type: object
        properties:
            lastname: 
                type: string
                required: true
                maxLength: 255
            firstname: string
    MapOfPerson:
        type: object
        properties:
            []:
                type: Person
    MapOfObject:
        type: object
        properties:
            []:
                type: object
                properties:
                    lastname: 
                        type: string
                        required: true
                        maxLength: 255
                    firstname: string
    MapOfPersonArray:
        type: object
        properties:
            []:
                type: Person[]
/persons:
  get:
    responses:
      200:
        body:
          type: MapOfPerson
  /map:
    get:
      responses:
        200:
          body:
            type: MapOfPersonArray
  post:
    description: adds a movie to the catalogue
    body:
      application/json:
        type: MapOfInt