#%RAML 1.0 DataType
type: object
properties: 
  id: 
    type: integer
    minimum: 1
    description: Moon Unique Identifier
    example: 1
  name:
    type: string
    description: The Moon Name
    example: "Phil Mac"
  size:
    type: integer
    description: The Moon Diameter expressed in Kilometers
    example: 8426