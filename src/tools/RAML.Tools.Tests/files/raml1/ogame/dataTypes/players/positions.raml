#%RAML 1.0 DataType
type: object
properties: 
  type:
    type: string
    description: Same filter as /highscore endpoint
    example: Honor
  score:
    type: integer
    description: Player score
    example: 12412425
  position:
    type: integer
    description: Player position
    example: 4
  ships:
    type: integer
    description: Player amount of ships. Only appears when type equals Military
    required: false
    example: 3200213