#%RAML 1.0 DataType

type: object
properties: 
  id: 
    type: integer
    minimum: 1
    description: Player Unique Identifier
    example: 1
  name:
    type: string
    description: Player Name
    example: Legor
  status:
    enum: [ a, i, I, v, vi, VI, vb, vib, vIb]
    description: The account status
  alliance:
    type: string
    description: Alliance Unique Identifier
    example: "500167"
    required: false